{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug emeocv",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/emeocv.exe",
            "args": [
                "-h"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build"
        },
        {
            "name": "Debug emeocv with test images",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/emeocv.exe",
            "args": [
                "-i",
                "test_images",
                "-t"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build"
        }
    ]
}
